<html>
<head>
<%=render "/layouts/header" %>
</head>
<body>
<div id="main">

  <h1>CIS Faculty</h1> </br>
  
  <% @faculties.each do |faculty| %>
    <%= image_tag faculty.image.url if faculty.image_stored? %>
    <p>Name: <%= faculty.name %></p> 
    <p>Title: <%= faculty.title %></p> 
    <p>Office: <%= faculty.office %></p>
    <p>Phone Number: <%= faculty.phone_number %></p>
    <p>Email: <%= faculty.email %></p>
	</br>
  <% end %>

  <br>

</div>
</body>
</html>