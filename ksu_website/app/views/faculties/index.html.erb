<html>
<head>
<%=render "/layouts/header" %>
</head>
<body>
<div id="main">

  <h1>Listing Faculty</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Title</th>
        <th>Office</th>
        <th>Phone number</th>
        <th>Email</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <% @faculties.each do |faculty| %>
        <tr>
          <td><%= faculty.name %></td>
          <td><%= faculty.title %></td>
          <td><%= faculty.office %></td>
          <td><%= faculty.phone_number %></td>
          <td><%= faculty.email %></td>        
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

</div>
</body>
</html>